[kelvins-server]
Environment = "dev"

[kelvins-logger]
RootPath = "./logs"
Level = "debug"

[kelvins-mysql]
Host = "mysql5_7:3306"
UserName = "root"
Password = "micro-mall"
DBName = "micro_mall"
Charset = "utf8mb4"
PoolNum =  10
MaxIdleConns = 5
ConnMaxLifeSecond = 3600
MultiStatements = true
ParseTime = true

[shop-info-search-notice]
Broker = "amqp://root:micro-mall@rabbitmq:5672/micro-mall"
DefaultQueue = "shop_info_search_notice"
ResultBackend = "redis://oyuioyiuoyui@redis:6379/8"
ResultsExpireIn = 3600
Exchange = "shop_info_search_notice"
ExchangeType = "direct"
BindingKey = "shop_info_search_notice"
PrefetchCount = 3
TaskRetryCount = 3
TaskRetryTimeout = 3600

[email-config]
Enable = false
User = "xxx@qq.com"
Password = "xxxx"
Host = "smtp.qq.com"
Port = "465"

[email-notice]
Receivers = "610905744@qq.com"

[shop-info-search-notice-task]
Cron = "0 */5 * * * *"
SingleSyncNum = 100